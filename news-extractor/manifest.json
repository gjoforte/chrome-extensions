{
  "manifest_version": 3,
  "name": "News Extractor",
  "version": "1.0",
  "description": "Extracts news content from specific element",
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }]
}
