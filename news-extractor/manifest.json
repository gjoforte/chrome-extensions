{
  "manifest_version": 3,
  "name": "News Extractor",
  "version": "1.0",
  "description": "Retrieve news content from blocked sites",
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://www.dn.pt/*",
        "https://www.tsf.pt/*"
      ]
    }
  ]
}
